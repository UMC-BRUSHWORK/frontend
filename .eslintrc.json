{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-props-no-spreading": ["off"],
    "react/prop-types": ["off"],
    "import/prefer-default-export": ["off"],
    "react/function-component-definition": [
      2,
      { "namedcomponents": "arrow-function" }
    ]
  }
}
